import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        for(int j=1; j<=N; j++){
            System.out.print(j+" ");
        }
        for(int i=N; i>0; i--){
            System.out.print(i+" "); 
        }
    }
}
